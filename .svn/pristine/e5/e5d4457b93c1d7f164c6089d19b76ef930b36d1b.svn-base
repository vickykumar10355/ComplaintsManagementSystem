
<div class="col-md-6 col-md-offset-3" >
    
	 <div class="row">
		 <div class="panel panel-default">
          
			  <div class="panel-heading">
                   
                  <h2>Sign in</h2>
              </div>
			  
			  <div class="panel-body">
					<%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
					 <div class="form-inputs">
					 
						<div class="form-group">
							<div class="input-group">
     						   <div class="input-group-addon">
                                   <span class="glyphicon glyphicon-user"></span> 
                               </div>
                               
                                <%= f.text_field :username, required: false, autofocus: true,:placeholder=>"Username", :class=>"form-control" %>
                           </div>
                       </div>
                       <div class="form-group">
                          <div class="input-group">
                              <div class="input-group-addon">
								  <span class="glyphicon glyphicon-lock"></span> 
                               </div>
                           
                              <%= f.password_field :password, required: false, :placeholder=>"Password", :class=>"form-control"  %>
                          </div>
					  </div>

					
					  
					 <div class="form-actions">
						<%= f.submit "Log in", class: "btn btn-primary center-block" %>
				     </div>
					  </div>
					<% end %>

					<%= render "devise/shared/links" %>
                  
			  </div>
            
              
		  </div>
         
        <%= video_tag "http://localhost:3000/videos/Produce_4.mp4", :size => "570x290", :controls => true, :autoplay => true, :loop=>true %>
         
       
         
	  </div>
    
  </div>
<nav class="navbar navbar-fixed-bottom">
  <div class="footer">
   
		<div class="container">
		 <div style="color:black;text-align:center;"> <h4>Complaints Management System enables citizens to register the issues they face in day-to-day life as complaints.</h4></div>
			  
	    </div>		  
	
 </div>
</nav>	 